{
  "name": "@foodtracks/custom-ui-components",
  "version": "0.1.7",
  "description": "Custom UI components",
  "main": "build/cjs/src/index.js",
  "repository": "https://github.com/rbalukja15/custom-ui-components.git",
  "author": "Romarjo Balukja <mbalukja1@gmail.com>",
  "license": "MIT",
  "files": [
    "build",
    "assets"
  ],
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:storybook": "npm run build:svg && build-storybook -c .storybook -o public/storybook",
    "build": "yarn run build:esm && yarn run build:cjs",
    "lint:fix": "eslint src --fix",
    "lint": "eslint src",
    "prebuild": "rimraf build/*",
    "storybook": "start-storybook -p 6006 -s build",
    "ts:watch": "concurrently \"yarn run build:esm -- -w\" \"yarn run build:cjs -- -w\"",
    "watch": "yarn run ts:watch",
    "test:ci": "npm run build:svg && CI=true TZ=UTC jest --no-cache",
    "test": "TZ=UTC jest --watch"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.12",
    "@types/ramda": "^0.28.23",
    "ramda": "^0.28.0",
    "react-scripts": "^5.0.1",
    "webfontloader": "^1.6.28"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-storysource": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@tsconfig/recommended": "^1.0.2",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^29.5.0",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.8.0",
    "babel-jest": "^29.5.0",
    "concurrently": "^7.6.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "storybook-addon-material-ui": "^0.9.0-alpha.24",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "webpack": "^5.76.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
